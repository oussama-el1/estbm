    <Label text="Add Cours" />
    <TextField fx:id="idEnseignantField" promptText="id Ensigniement" />
    <TextField fx:id="nomCoursField" promptText="Nom cours" />
    <Button styleClass="button" text="Submit" onAction="#handleaddSubmit" />
</VBox>

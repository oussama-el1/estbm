<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:id="mainContentPane" fx:controller="CourslistEnseignantController" spacing="10" stylesheets="@../style/listStyles.css" alignment="TOP_CENTER">
    <TableView fx:id="coursTable">
        <columns>
            <TableColumn text="ID" fx:id="idCoursColumn" />
            <TableColumn text="Nom du Cours" fx:id="nomCoursColumn" />
            <TableColumn text="Nom de l'Enseignant" fx:id="nomEnseignantColumn" />
        </columns>
    </TableView>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.event.ActionEvent?>

<StackPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:id="mainContentPane" fx:controller="ExamensController" stylesheets="@../style/EnseignantsForm.css" >
    <VBox alignment="TOP_CENTER" spacing="10">
        <HBox styleClass="top-bar">
            <Label styleClass="label-title" text="Etudiants Form" />
            <Button styleClass="button" text="Add" onAction="#handleAdd" />
            <Button styleClass="button" text="Update" onAction="#handleUpdate" />
            <Button styleClass="button" text="Remove" onAction="#handleRemove" />
        </HBox>
    </VBox>
</StackPane>